<script>
var scroll_x = 0

$('.submit').click( function() {
  var name = $('#name-homepage').val() 
  var email = $('#email-homepage').val() 
  var message = $('.email-message').val() 
  var captcha_response = $('.g-recaptcha-response').val()
  $.ajax({
    type: "POST",
    data: {name: name, email: email, message: message, g_response: captcha_response},
    url: "/send_email",
    success: function(msg){
      alert(msg["message"]);
    }
  });
})


$('.right-arrow-1').click( function() {
  scroll_x = scroll_x + 1000;
  $(".portfolio-1").animate({
      scrollLeft: scroll_x
    }, 2000);
})

$('.left-arrow-1').click( function() {
  scroll_x = scroll_x - 1000;
  $(".portfolio-1").animate({
      scrollLeft: scroll_x
    }, 2000);
})

$('.right-arrow-2').click( function() {
  scroll_x = scroll_x + 1000;
  $(".portfolio-2").animate({
      scrollLeft: scroll_x
    }, 2000);
})

$('.left-arrow-2').click( function() {
  scroll_x = scroll_x - 1000;
  $(".portfolio-2").animate({
      scrollLeft: scroll_x
    }, 2000);
})
</script>